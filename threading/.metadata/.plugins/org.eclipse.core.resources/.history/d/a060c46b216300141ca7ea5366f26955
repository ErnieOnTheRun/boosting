template<typename T>
	class Queue {

	public:
	Queue() {};
	~Queue() {};
/*
 * add an element x to the queue
 */
	void enqueue (const T& x){

		boost::mutex::scoped_lock(mutex_);
		list_.push_back(x);


	}



};

int main(){

boost::thread myThread(threadFun);

boost::thread::yield();

myThread.join();

